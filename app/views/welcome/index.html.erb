<% if current_user %>
    <%= link_to "Log Out", logout_path, method: :get %><br>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= link_to user.email, dashboard_path(user.id) %></li>
        <% end %>
    </section>
<% else %>
    <%= button_to "Create an Account", register_path, method: :get %><br>
    <%= link_to "Log In", login_path, method: :get %>
<% end %>